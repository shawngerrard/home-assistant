{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "TS Pulumi k3s-infra index.ts",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["--nolazy","-r","ts-node/register"],
      "program": "${workspaceFolder}/index.ts",
      "preLaunchTask": "npm: build",
      "dap-compilation": "npm run build",
      "outFiles": ["${workspaceFolder}/dist/*.js"],
      "protocol": "inspector"
    }
  ]
}
